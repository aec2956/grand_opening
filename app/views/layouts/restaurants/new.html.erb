<%= form_for @restaurants do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.label :address %>
  <%= f.text_field :address%>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <%= f.label :zipcode %>
  <%= f.text_field :zipcode %>
  <%= f.label :phone %>
  <%= f.text_field :phone %>
  <%= f.submit %>
  <% end %>

