<%= simple_form_for @restaurant do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :address %>
  <%= f.input :city %>
  <%= f.input :state %>
  <%= f.input :zipcode %>
  <%= f.input :telephone %>
  <%= f.input :hours %>
  <%= f.input :region, collection: @regions %>
  
  <%= f.submit %>
  <% end %>

