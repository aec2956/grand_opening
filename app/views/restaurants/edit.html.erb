<div class="booyah-box col-10 offset-1">

<h1>Edit Your Location</h1>
<%= simple_form_for @location do |f| %>
    <%= f.input :address %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :zipcode %>
  
    <br />
    <%= f.submit 'Update', class: 'btn btn-primary' %>
  <% end %>
</div>